{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "env": {
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "globals": {
    "Promise": true,
    "Symbol": true,
    "Proxy": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "no-empty": 0,
    "semi": 0,
    "eol-last": 0
  }
}
